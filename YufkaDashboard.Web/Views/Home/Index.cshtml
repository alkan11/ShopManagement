	<script>
		function ShowPaymentTypeDiv()
		{
			const repeater = document.getElementById("repeater_basic");
			repeater.classList.add("repeater-locked");
			const inputs = document.querySelectorAll(".form-repeater input, .form-repeater select");
			inputs.forEach((element) => {
				element.classList.add("locked"); // Gri görünüm için sınıf ekle
				element.disabled = true; // Alanı devre dışı bırak
			});
			$("#PaymentTypeDiv").show();
		}
	</script>
	<script>
		window.onload = function (){
			$('#repeater_basic').repeater({
				
				initEmpty: false,

				defaultValues: {
					'text-input': 'foo'
				},

				show: function () {
					$(this).slideDown();
				},

				hide: function (deleteElement) {
					$(this).slideUp(deleteElement);
				}
			});
		}
		
	</script>
	<script>
		document.addEventListener("DOMContentLoaded", function () {

			
			// Tüm repeater item'larını izle
			document.querySelector('#repeater_basic').addEventListener('change', function (e) {
				const target = e.target;
				const targetName=target.name;
				var productUnitPrice=0;
				var index=0;

				const formElements = document.querySelectorAll('input, select');

				// Her öğeye tıklama olayı ekliyoruz
				formElements.forEach(element => {
					element.addEventListener('click', function () {
						// Tıklanan öğenin 'name' attribute'unu alıyoruz
						const nameValue = element.name;
						index=nameValue.split('[')[1].split(']')[0];

					});
				});

				const selectElement = document.querySelector('select[name="Baskets[' + index + '][ProductId]"]');
				var selectProductValue=selectElement.value;
				// change olayı dinleyici ekliyoruz
				selectElement.addEventListener('change', function () {
					// Seçilen productId'yi alıyoruz, ilk değer de dahil
					 selectProductValue = selectElement.value;
					console.log("Selected productId: ", selectProductValue);  // Seçilen değeri logluyoruz
					
				});
				if(selectProductValue){
					 //Eğer gerekli API çağrısını yapacaksa, burada işlemi yapabilirsiniz
					fetch('/Home/GetProductPrice?productId=' + selectProductValue)
					     .then(response => response.json())
					     .then(data => {
					          productUnitPrice = data.unitPrice;
					     })
					     .catch(error => {
					         console.error('ürün Fiyat Alımında Js Hatası:', error);
					     });
				}
				
				

				 //Ürün seçildiğinde fiyatı getir
				//if (target && target.name === 'Baskets['+index+'][ProductId]') {

				//	const productId = target.value;

				//	fetch('/Home/GetProductPrice?productId=' + productId)
				//		.then(response => response.json())
				//		.then(data => {
							
				//			productUnitPrice = data.unitPrice;
				//		});

				//}

				const inputFieldAmount = document.querySelector('input[name="Baskets['+index+'][Amount]"]');
				const inputFieldPrice = document.querySelector('input[name="Baskets[' + index + '][Price]"]');

				// Input alanındaki değişiklikleri dinliyoruz
				inputFieldAmount.addEventListener("input", function (event) {
					// Girilen değeri alıyoruz
					let enteredValue = inputFieldAmount.value;
					if(!enteredValue){enteredValue=0}
					inputFieldPrice.value=productUnitPrice*enteredValue
				});
			});
		});
	</script>
